{
    "name": "visual-notes-cli-ingest",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "bin": {
        "visual-notes-ingest": "dist/cli.js"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "dev": "tsx src/cli.ts",
        "start": "node dist/cli.js",
        "ingest": "tsx src/cli.ts --markdown --split --post -v",
        "export:compile": "osacompile -l JavaScript -o scripts/export-apple-notes.scpt scripts/export-apple-notes.jxa",
        "export:10": "LIMIT=10 osascript scripts/export-apple-notes.jxa"
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "turndown": "^7.2.0",
        "commander": "^12.1.0",
        "zod": "^4.1.12",
        "consola": "^3.2.3",
        "execa": "^8.0.1",
        "cheerio": "^1.0.0-rc.12"
    },
    "devDependencies": {
        "@types/node": "^22.7.5",
        "tsx": "^4.19.1",
        "typescript": "^5.6.3"
    }
}